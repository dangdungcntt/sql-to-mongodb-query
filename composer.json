{
    "name": "nddcoder/sql-to-mongodb-query",
    "description": "",
    "keywords": [
        "nddcoder",
        "sql-to-mongodb-query"
    ],
    "homepage": "https://github.com/nddcoder/sql-to-mongodb-query",
    "license": "MIT",
    "authors": [
        {
            "name": "Dung Nguyen Dang",
            "email": "dangdungcntt@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "mongodb/mongodb": "^1.8",
        "phpmyadmin/sql-parser": "^4.7",
        "ext-mongodb": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "symfony/var-dumper": "^5.2",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Nddcoder\\SqlToMongodbQuery\\": "src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Nddcoder\\SqlToMongodbQuery\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "psalm",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
